<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/startup_adv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startup adv" />

    <Button
        android:id="@+id/banner_adv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="banner adv" />

    <Button
        android:id="@+id/appinstall_adv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="appinstall adv" />

    <Button
        android:id="@+id/checkAndUpdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="app check and update" />

</android.support.constraint.ConstraintLayout>